-- Config
local FLIGHT_SPEED = 50
local TOGGLE_KEY = Enum.KeyCode.L
local ASCEND_KEY = Enum.KeyCode.Space
local DESCEND_KEY = Enum.KeyCode.LeftShift

-- Flight Toggle Button
local FlightToggle = MiscTab:CreateToggle({
    Name = "ðŸ•Šï¸ Flight (Press L)",
    CurrentValue = false,
    Flag = "FlightToggle",
    Callback = function(Value)
        flying = Value
        toggleFlight()
    end
})

-- Flight Speed Slider
local FlightSpeedSlider = MiscTab:CreateSlider({
    Name = "Flight Speed",
    Range = {10, 200}, -- à¸à¸³à¸«à¸™à¸”à¸Šà¹ˆà¸§à¸‡à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§
    Increment = 5, -- à¹€à¸žà¸´à¹ˆà¸¡à¸—à¸µà¸¥à¸° 5
    CurrentValue = FLIGHT_SPEED, -- à¸„à¹ˆà¸²à¹€à¸£à¸´à¹ˆà¸¡à¸•à¹‰à¸™
    Flag = "FlightSpeedValue",
    Callback = function(Value)
        FLIGHT_SPEED = Value -- à¸­à¸±à¸›à¹€à¸”à¸•à¸„à¹ˆà¸²à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¸ˆà¸£à¸´à¸‡
    end
