local DestroyUI = SettingsTab:CreateButton({
   Name = "Destroy UI",
   Callback = function()
      CleanupESP("skeleton")
      CleanupESP("arrows")
      CleanupESP("radar")
      Rayfield:Destroy()
   end,
})

