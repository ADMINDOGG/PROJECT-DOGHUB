local CreditsLabel = SettingsTab:CreateLabel("Created by: DogTeam")
local CreditsLabel = SettingsTab:CreateLabel("join discord: https://discord.gg/TBkcu6rccJ")
local VersionLabel = SettingsTab:CreateLabel("Version: 1.3.3")
